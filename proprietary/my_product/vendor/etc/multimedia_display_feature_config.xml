<?xml version="1.0" encoding="UTF-8"?>

<filter-conf>
    <version>20220721</version>
    <toggle>1</toggle>
    <features>
        <feature name="hecate" enable="1"/>
        <feature name="OnlineAband" enable="1"/>
        <feature name="DispKeventThread" enable="1"/>
        <feature name="watchdog" enable="1"/>
        <feature name="DisplayBoost" enable="1"/>
        <feature name="OplusDEE" enable="1">
            <application name="com.coloros.video">
                <scene type="Application">
                    <action>
                        <mode>SVI</mode>
                    </action>
                </scene>
            </application>
            <application name="com.oplus.camera">
                <scene type="Application">
                    <action>
                        <mode>SVI</mode>
                    </action>
                </scene>
            </application>
            <application name="com.coloros.gallery3d">
                <scene type="Application">
                    <action>
                        <mode>SVI</mode>
                    </action>
                </scene>
            </application>
        </feature>
    </features>
</filter-conf>
